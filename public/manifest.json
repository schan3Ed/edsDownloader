{
  "manifest_version": 2,
  "name": "WolfFile",
  "description": "This chrome extension helps user to better organize files from wolfware",
  "version": "1.0.0",
  "version_name": "beta",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "downloads", "*://*/*",
    "tabs",
    "https://ajax.googleapis.com/",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon.png"
  }    
}