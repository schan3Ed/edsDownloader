{
  "manifest_version": 2,

  "name": "EdsDownload", 
  "description": "This Extension helps user automatically organise their download item from Wolfware",
  "version": "0.1.3",

  "browser_action": {
      "default_popup": "popup.html"
    },
  "background": {"persistent": false, "scripts": ["background.js"]}, 
   
  "permissions": [
    "activeTab", "downloads", "tabs",
    "https://ajax.googleapis.com/", "storage"
  ]
}

